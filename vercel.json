{
  "version": 2,
  "functions": {
    "api/screenshot.js": {
      "runtime": "@vercel/node@3"
    },
    "index.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/screenshot",
      "dest": "/api/screenshot.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}