{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/screenshot",
      "dest": "/api/screenshot.js"
    },
    {
      "src": "/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}